{
  "name": "@srclaunch/react-hooks",
  "description": "React hooks used in AppLab applications",
  "author": "Steven Bennett <steven@srclaunch.com>",
  "license": "MIT",
  "version": "0.1.7",
  "engines": {
    "node": ">=16"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "type": "module",
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "package.json",
    "dist"
  ],
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.umd.js"
    }
  },
  "scripts": {
    "build": "yarn srclaunch build",
    "qr": "yarn build && yarn srclaunch changesets add --message \"Quick release - patch\" && yarn srclaunch release",
    "release": "yarn test:coverage && yarn srclaunch release",
    "test": "yarn srclaunch test",
    "test:watch": "yarn srclaunch test --watch",
    "test:coverage": "yarn srclaunch test --coverage",
    "yui": "yarn upgrade-interactive"
  },
  "devDependencies": {
    "@srclaunch/cli": "^0.1.292",
    "@srclaunch/dx": "^0.1.44",
    "@types/react": "^18.0.7",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "@vitejs/plugin-react": "^1.3.1",
    "esbuild": "^0.14.38",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-config-stylelint": "^15.0.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-functional": "^4.2.1",
    "eslint-plugin-github": "^4.3.6",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-only-warn": "^1.0.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.4.0",
    "eslint-plugin-regexp": "^1.7.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-plugin-sort-requires": "^2.1.0",
    "eslint-plugin-testing-library": "^5.3.1",
    "eslint-plugin-unicorn": "^42.0.0",
    "prettier": "^2.6.2",
    "prettier-eslint": "^14.0.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-router-dom": "^6.3.0",
    "standard-version": "^9.3.2",
    "stylelint": "^14.7.1",
    "stylelint-config-idiomatic-order": "^8.1.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended": "^7.0.0",
    "stylelint-order": "^5.0.0",
    "typescript": "^4.6.3",
    "vite": "^2.9.5"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.1"
  }
}
